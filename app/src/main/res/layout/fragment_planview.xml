<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_viewmodel"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        app:labelVisibilityMode="labeled"
        android:layout_marginEnd="0dp"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/white"
        android:background="@color/purple_500"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_viewmodel"/>
    <fragment
        android:id="@+id/fragmentviewmodel"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/nav_viewmodel"
        app:layout_constraintTop_toTopOf="parent"
        app:defaultNavHost="true"
        app:navGraph="@navigation/navgraph3" />
    <TextView
        android:id="@+id/tv_fab1viewmodel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="8dp"
        android:elevation="2dp"
        android:fontFamily="sans-serif"
        android:padding="5dip"
        app:layout_constraintTop_toTopOf="@id/fab_planviews"
        app:layout_constraintBottom_toBottomOf="@id/fab_planviews"
        app:layout_constraintEnd_toStartOf="@id/fab_planviews"
        android:text="Viewplans"
        android:textColor="@android:color/black"
        android:typeface="normal" />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_downloadviemodel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabSize="mini"
        app:tint="@color/white"
        app:layout_constraintEnd_toEndOf="@id/fab_mainviewmdoel"
        app:layout_constraintBottom_toTopOf="@id/nav_viewmodel"
        android:layout_marginBottom="16dp"
        app:layout_constraintStart_toStartOf="@id/fab_mainviewmdoel"
        android:src="@drawable/ic_action_download"
        android:layout_gravity="center"/>

    <TextView
        android:id="@+id/tv_fab3viewmodel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="8dp"
        android:elevation="2dp"
        android:fontFamily="sans-serif"
        android:padding="5dip"
        app:layout_constraintTop_toTopOf="@id/fab_downloadviemodel"
        app:layout_constraintBottom_toBottomOf="@id/fab_downloadviemodel"
        app:layout_constraintEnd_toStartOf="@id/fab_downloadviemodel"
        android:text="Download"
        android:textColor="@android:color/black"
        android:typeface="normal" />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_editviewmodel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabSize="mini"
        app:tint="@color/white"
        app:layout_constraintEnd_toEndOf="@id/fab_mainviewmdoel"
        app:layout_constraintBottom_toTopOf="@id/nav_viewmodel"
        android:layout_marginBottom="16dp"
        app:layout_constraintStart_toStartOf="@id/fab_mainviewmdoel"
        android:src="@drawable/ic_action_edit"
        android:layout_gravity="center"/>

    <TextView
        android:id="@+id/tv_fab2viewmodel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="8dp"
        android:elevation="2dp"
        android:fontFamily="sans-serif"
        android:padding="5dip"
        app:layout_constraintTop_toTopOf="@id/fab_editviewmodel"
        app:layout_constraintBottom_toBottomOf="@id/fab_editviewmodel"
        app:layout_constraintEnd_toStartOf="@id/fab_editviewmodel"
        android:text="Edit"
        android:textColor="@android:color/black"
        android:typeface="normal" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_planviews"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabSize="mini"
        app:tint="@color/white"
        app:layout_constraintEnd_toEndOf="@id/fab_mainviewmdoel"
        app:layout_constraintBottom_toTopOf="@id/nav_viewmodel"
        android:layout_marginBottom="16dp"
        app:layout_constraintStart_toStartOf="@id/fab_mainviewmdoel"
        android:src="@drawable/ic_action_models"
        android:layout_gravity="center"/>


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/nav_viewmodel"
        android:layout_marginBottom="16dp"
        android:id="@+id/fab_mainviewmdoel"
        app:tint="@color/white"
        android:src="@drawable/ic_action_edit"
        android:layout_marginEnd="16dp"/>



    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomnavigationplanview"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        app:labelVisibilityMode="labeled"
        android:layout_marginEnd="0dp"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/white"
        android:background="@color/purple_500"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_menu">

    </com.google.android.material.bottomnavigation.BottomNavigationView>


    <fragment
        android:id="@+id/fragmentContainerView2"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/bottomnavigationplanview"
        app:defaultNavHost="true"
        app:navGraph="@navigation/navgraph2" />

    <TextView
        android:id="@+id/tv_fab1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="8dp"
        android:elevation="2dp"
        android:fontFamily="sans-serif"
        android:padding="5dip"
        app:layout_constraintTop_toTopOf="@id/fab_viewmodels"
        app:layout_constraintBottom_toBottomOf="@id/fab_viewmodels"
        app:layout_constraintEnd_toStartOf="@id/fab_viewmodels"
        android:text="Viewmodels"
        android:textColor="@android:color/black"
        android:typeface="normal" />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_download"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabSize="mini"
        app:tint="@color/white"
        app:layout_constraintEnd_toEndOf="@id/fab_mainmenu"
        app:layout_constraintBottom_toTopOf="@id/bottomnavigationplanview"
        android:layout_marginBottom="16dp"
        app:layout_constraintStart_toStartOf="@id/fab_mainmenu"
        android:src="@drawable/ic_action_download"
        android:layout_gravity="center"/>

    <TextView
        android:id="@+id/tv_fab3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="8dp"
        android:elevation="2dp"
        android:fontFamily="sans-serif"
        android:padding="5dip"
        app:layout_constraintTop_toTopOf="@id/fab_download"
        app:layout_constraintBottom_toBottomOf="@id/fab_download"
        app:layout_constraintEnd_toStartOf="@id/fab_download"
        android:text="Download"
        android:textColor="@android:color/black"
        android:typeface="normal" />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabSize="mini"
        app:tint="@color/white"
        app:layout_constraintEnd_toEndOf="@id/fab_mainmenu"
        app:layout_constraintBottom_toTopOf="@id/bottomnavigationplanview"
        android:layout_marginBottom="16dp"
        app:layout_constraintStart_toStartOf="@id/fab_mainmenu"
        android:src="@drawable/ic_action_edit"
        android:layout_gravity="center"/>

    <TextView
        android:id="@+id/tv_fab2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="8dp"
        android:elevation="2dp"
        android:fontFamily="sans-serif"
        android:padding="5dip"
        app:layout_constraintTop_toTopOf="@id/fab_edit"
        app:layout_constraintBottom_toBottomOf="@id/fab_edit"
        app:layout_constraintEnd_toStartOf="@id/fab_edit"
        android:text="Edit"
        android:textColor="@android:color/black"
        android:typeface="normal" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_viewmodels"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabSize="mini"
        app:tint="@color/white"
        app:layout_constraintEnd_toEndOf="@id/fab_mainmenu"
        app:layout_constraintBottom_toTopOf="@id/bottomnavigationplanview"
        android:layout_marginBottom="16dp"
        app:layout_constraintStart_toStartOf="@id/fab_mainmenu"
        android:src="@drawable/ic_action_models"
        android:layout_gravity="center"/>


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/bottomnavigationplanview"
        android:layout_marginBottom="16dp"
        android:id="@+id/fab_mainmenu"
        app:tint="@color/white"
        android:src="@drawable/ic_action_edit"
        android:layout_marginEnd="16dp"/>




</androidx.constraintlayout.widget.ConstraintLayout>