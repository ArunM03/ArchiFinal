<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.codezcook.archiplanner.ui.dashboard.fragments.ImageActivity">

    <com.ortiz.touchview.TouchImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/touchimage_iv"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/waterlogo"
        android:id="@+id/iv_waterlogo_model"
        app:layout_constraintStart_toStartOf="@id/touchimage_iv"
        app:layout_constraintEnd_toEndOf="@id/touchimage_iv"
        app:layout_constraintTop_toTopOf="@id/touchimage_iv"
        app:layout_constraintBottom_toBottomOf="@id/touchimage_iv"/>

</androidx.constraintlayout.widget.ConstraintLayout>